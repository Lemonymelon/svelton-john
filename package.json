{
	"name": "svelton-john",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"tree": "sh -c 'find . \\( -name \"node_modules\" -o -name \"dist\" -o -name \".git\" \\) -prune -o -type f -print > temp.txt && clip < temp.txt && rm temp.txt'",
		"migrate:make": "npx ts-node src/db/knex-commands.ts migrate:make",
		"migrate:latest": "npx ts-node src/db/knex-commands.ts migrate:latest",
		"migrate:rollback": "npx ts-node src/db/knex-commands.ts migrate:rollback"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.22",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"@types/knex": "^0.16.1",
		"@types/node": "^22.5.0",
		"@types/pg": "^8.11.6",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.36.0",
		"esm": "^3.2.25",
		"globals": "^15.0.0",
		"knex": "^3.1.0",
		"postcss": "^8.4.41",
		"svelte": "^4.2.18",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.4.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4",
		"vite": "^5.4.0"
	},
	"dependencies": {
		"@langchain/core": "^0.2.0",
		"@langchain/openai": "^0.2.0",
		"@supabase/supabase-js": "^2.45.1",
		"dotenv": "^16.4.5",
		"openai": "^4.55.7",
		"pg": "^8.12.0",
		"svelte-hero-icons": "^5.2.0"
	}
}