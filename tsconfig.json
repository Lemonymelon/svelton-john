{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"$env/static/private": [
				"node_modules/@sveltejs/kit/types/env/static/private"
			],
			"$env/static/public": [
				"node_modules/@sveltejs/kit/types/env/static/public"
			],
			"$env/dynamic/private": [
				"node_modules/@sveltejs/kit/types/env/dynamic/private"
			],
			"$env/dynamic/public": [
				"node_modules/@sveltejs/kit/types/env/dynamic/public"
			]
		}
	},
	"include": [
		"src/**/*.ts",
		".svelte-kit/ambient.d.ts",
		".svelte-kit/types/**/$types.d.ts"
	],
	"exclude": [
		"node_modules"
	],
}